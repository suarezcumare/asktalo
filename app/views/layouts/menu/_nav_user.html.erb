<header>
  <nav class="navbar  nav-color">
    <div class="container-fluid">
      <div class="col-md-offset-8 navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <div class="logo-nav"  href="#">
          <img src="/assets/asktalo-logo.png" class="img-logo"/>
        </div>
      </div>

      <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
        <ul class="nav navbar-nav navbar-right">
          <li class="col-lg-3 col-md-3 col-xs-2">  <%= link_to 'home', root_path, :class => 'navbar-link' %> </li>
          <li class="col-lg-3 col-md-3 col-xs-2">
          <a  class="dropdown-toggle" data-toggle="dropdown" aria-expanded="true">
            <div class="logged-in-usuario">
              <div class="media-left">
            <% if current_user && current_user.id != nil %>
                      <img class="img-circle" src="<%= current_user.picture %>"  alt=""/>
                 <% else %>
                      <img class="img-circle" src="/assets/avatar/user1.jpg" alt=""/>
                 <% end %>
              </div>
              <div class="media-body">
                <h6 class="media-heading"><%= current_user.full_name %> </h6>
              </div>
            </div>
          </a>
          <ul class="dropdown-menu" role="menu" aria-labelledby="dropdownMenu1">
            <li>  <%= link_to 'Setting', edit_user_registration_path, :class => 'navbar-link' %> </li>
            <li role="presentation"><%= link_to "Logout", destroy_user_session_path,method: :delete, :class => 'navbar-link'  %></li>
          </ul>
        </li>
        </ul>
      </div>
    </div>
  </nav>
</header>
